<h1 class="white">Listado de Productos!</h1>
<%@products.each do |product|%>
  <h3 class="white"><%=product.name %>-<%= link_to"Eliminar" ,product,:confirm => "Seguro desea eliminar este Producto?",method: :delete%></h3>
  <h3 class="white"><%=product.brand %></h3>
  <h3 class="white"><%=product.description %></h3>
  <h3 class="white"><%=product.price %>-</h3>


  <%end%>
